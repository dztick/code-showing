-- Tes capability Solara
local ok = {}
ok.getrawmetatable = (type(getrawmetatable) == "function")
ok.newcclosure = (type(newcclosure) == "function")
ok.setreadonly = (type(setreadonly) == "function")
ok.hookmetamethod = (type(hookmetamethod) == "function") -- alternatif hook
local t = "Capabilities:\n"
for k,v in pairs(ok) do t = t..string.format("%s = %s\n", k, tostring(v)) end
-- tampil di screen karena Solara kadang tanpa console
local p = Instance.new("ScreenGui", game.Players.LocalPlayer:WaitForChild("PlayerGui"))
p.ResetOnSpawn = false
local l = Instance.new("TextLabel", p)
l.Size = UDim2.new(0,400,0,100)
l.Position = UDim2.new(0,0,0,50)
l.TextScaled = false
l.TextWrapped = true
l.Text = t
task.delay(6, function() p:Destroy() end)
print(t)
